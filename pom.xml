		<servlet.version>3.0.1</servlet.version>
		<servlet.api.version>2.5</servlet.api.version>
		<jsp.api.version>2.2</jsp.api.version>
		<cglib.version>2.2.2</cglib.version>
		<org.tiles.version>2.2.2</org.tiles.version>
